<div class="container">
    <div class="row profile-header">
        <h3>My Contacts</h3>
    </div>
    <hr>
    <div class="carousel container">

      {{#each data}}
        <div class="carousel-item">
          <div class="carousel-fixed-item center">
            <h4>{{this.name}}</h4>
            <h6>{{this.where_met}}</h6>
            <span class="stars center">{{this.rating}}</span>
          </div>
          <a href="/suitor/{{this.id}}">
          <img class="circle responsive-img" src="{{this.image_url}}">
          </a>

        </div>

      {{/each}}
    </div>
    <button class="btn blue" id="add_suitor"type="submit" name="add_suitor">Add New Contact</button>
    <form class="hidden_form" action="{{user.id}}/addnew" method="post">

    </form>
    <div class="add-suitor container">
      <button class="btn blue" id="add_suitor"type="submit" name="add_suitor">Add New Contact</button>
      <form class="hidden_form" action="{{user.id}}/addnew" method="post">
    </div>
    </form>
</div>
